    "alvearium-js-darwin-arm64": "0.0.1",
    "alvearium-js-android-arm64": "0.0.1",
    "alvearium-js-linux-arm64-gnu": "0.0.1",
    "alvearium-js-linux-arm64-musl": "0.0.1",
    "alvearium-js-win32-arm64-msvc": "0.0.1",
    "alvearium-js-linux-arm-gnueabihf": "0.0.1",
    "alvearium-js-linux-x64-musl": "0.0.1",
    "alvearium-js-freebsd-x64": "0.0.1",
    "alvearium-js-win32-ia32-msvc": "0.0.1",
    "alvearium-js-android-arm-eabi": "0.0.1"
